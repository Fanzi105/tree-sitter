void main(void)//切换界面才进行保存
{
		vu16 k=0;
    LCD_Init();//液晶初始化
	Usart_Config(1,9600);//串口1初始化
		Timer0_Init();//定时器0初始化
		#ifdef stc_1T
			Usart_Config(2,0);//串口1初始化
		#endif
		SysRead();//系统读取参数
	
		while(1)
    {
			sendchar(1,'a');
			delayms(100);
		}
}
//定时器中断,1ms
void timer0() interrupt 1
{
	TH0=(65536-1000)/256;
	TL0=(65536-1000)%256;	
	uartint();

}
